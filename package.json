{
  "name": "Internet Exploring",
  "description": "What if the Internet was your escape room?",
  "version": "0.1.0",
  "main": "public/electron.js",
  "author": "Domagoj Zganec",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "canvas-confetti": "^1.5.1",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "firebase": "^9.8.4",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "steamworks.js": "^0.0.25",
    "styled-components": "^5.3.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "electron": "wait-on tcp:3000 && electron-forge start",
    "start:dev": "REACT_APP_STAGE=dev concurrently -k \"BROWSER=none react-app-rewired start\" \"npm:electron\"",
    "start:prod": "REACT_APP_STAGE=prod concurrently -k \"BROWSER=none react-app-rewired start\" \"npm:electron\"",
    "build:prod": "REACT_APP_STAGE=prod react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "prettier": "npx prettier --write ./src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.1",
    "concurrently": "^7.5.0",
    "electron": "^21.2.3",
    "react-app-rewired": "^2.2.1",
    "wait-on": "^6.0.1"
  },
  "rescripts": [
    "env"
  ]
}
